fn main() -> i32 {
    let offset: i32 = 10;
    let add_offset: fn(i32) -> i32 = |x| x + offset;
    let a: i32 = add_offset(5);
    if a != 15 { return 1; }

    let base: i32 = 2;
    let multiplier: i32 = 10;
    let compute: fn(i32) -> i32 = |x| x * base + multiplier;
    let b: i32 = compute(5);
    if b != 20 { return 2; }

    0
}
