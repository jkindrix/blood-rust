// Test associated type resolution in impl blocks

struct IntBox {
    value: i32,
}

// Impl block with associated type
impl IntBox {
    type Item = i32;

    fn get(&self) -> Self::Item {
        self.value
    }
}

fn main() {
    let box1 = IntBox { value: 42 };
    let val: i32 = box1.get();
    println!("{}", val);
}
