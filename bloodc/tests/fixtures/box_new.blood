// Test Box::new builtin method

pub struct Inner {
    pub x: i32,
    pub y: i32,
}

pub enum Container {
    Empty,
    Simple(Inner),
    Boxed {
        inner: Box<Inner>,
        name: i32,
    },
}

fn test_box_new() {
    // Basic Box::new usage
    let inner = Inner { x: 1, y: 2 };
    let boxed = Box::new(inner);

    // Box in struct variant
    let inner2 = Inner { x: 3, y: 4 };
    let container = Container::Boxed {
        inner: Box::new(inner2),
        name: 42,
    };
}

fn main() {
    test_box_new();
}
