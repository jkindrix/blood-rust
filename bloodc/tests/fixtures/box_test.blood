// Test Box functionality

fn main() {
    // Test Box::new with integer
    let b = Box::new(42);
    println!("Created a Box");

    // Test Box::as_ref
    let r = b.as_ref();
    println!("Got reference from Box");

    // Test Box::new with a struct-like tuple
    let pair = Box::new((1, 2));
    println!("Created a Box with a pair");
}
