// Test using various keywords as struct field names.
// Some keywords should be contextual and work as field names.

struct EffectConfig {
    // Effect system keywords as field names
    effect: i32,
    handler: i32,
    op: i32,
    default: i32,
    handle: i32,
    deep: i32,
    shallow: i32,
    pure: i32,
    resume: i32,
    perform: i32,
}

struct CommonConfig {
    // Common keywords as field names
    type: i32,
    in: i32,
    async: i32,
    await: i32,
    move: i32,
    ref: i32,
    with: i32,
    where: i32,
}

pub fn main() -> i32 {
    let ec = EffectConfig {
        effect: 1,
        handler: 2,
        op: 3,
        default: 4,
        handle: 5,
        deep: 6,
        shallow: 7,
        pure: 8,
        resume: 9,
        perform: 10,
    };

    let cc = CommonConfig {
        type: 1,
        in: 2,
        async: 3,
        await: 4,
        move: 5,
        ref: 6,
        with: 7,
        where: 8,
    };

    ec.effect + cc.type
}
