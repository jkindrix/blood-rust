enum Inner { Val(i32), Empty }
struct Container { pub kind: Inner }
enum Outer { Wrap(Container), None }
fn main() -> i32 {
    let inner = Inner::Val(42);
    let c = Container { kind: inner };
    let outer = Outer::Wrap(c);
    match outer {
        Outer::Wrap(container) => {
            match container.kind {
                Inner::Val(v) => v,
                Inner::Empty => 99,
            }
        }
        Outer::None => 99,
    }
}
