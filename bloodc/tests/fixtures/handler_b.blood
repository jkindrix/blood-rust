
effect Counter {
    op inc() -> i32;
}

deep handler HandlerB for Counter {
    let mut count: i32
    return(x) { x + 100 }  // Different return clause!
    op inc() {
        count = count + 1;
        resume(count)
    }
}

fn main() -> i32 {
    with HandlerB { count: 0 } handle {
        perform Counter.inc()
    }
}
