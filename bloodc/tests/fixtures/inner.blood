// Inner module with enum variants that have the same names as structs in parent
//
// This module is used by cross_module_name_collision.blood to test that
// these enum variant names don't incorrectly shadow parent struct definitions.

pub enum DefKind {
    // These variant names intentionally match struct names in the parent module
    Variant,
    AssocType,
    Function,
    Module,
}

pub fn get_kind() -> DefKind {
    DefKind::Variant
}
