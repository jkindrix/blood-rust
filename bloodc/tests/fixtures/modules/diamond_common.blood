// Shared types for diamond dependency test
// This module is imported by both left.blood and right.blood

pub struct Span {
    pub start: usize,
    pub end: usize,
}

pub struct Token {
    pub kind: i32,
    pub span: Span,
}

pub fn make_span(start: usize, end: usize) -> Span {
    Span { start, end }
}
