// Left branch of diamond dependency
// Imports diamond_common.blood
mod diamond_common;

pub struct LeftNode {
    pub span: diamond_common::Span,
    pub value: i32,
}

pub fn make_left_node(span: diamond_common::Span, value: i32) -> LeftNode {
    LeftNode { span, value }
}

pub fn get_span_from_left(node: LeftNode) -> diamond_common::Span {
    node.span
}
