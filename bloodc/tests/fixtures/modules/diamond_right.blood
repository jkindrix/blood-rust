// Right branch of diamond dependency
// Imports diamond_common.blood (same module as diamond_left.blood imports)
mod diamond_common;

pub struct RightNode {
    pub span: diamond_common::Span,
    pub name: i32,
}

pub fn make_right_node(span: diamond_common::Span, name: i32) -> RightNode {
    RightNode { span, name }
}

pub fn get_span_from_right(node: RightNode) -> diamond_common::Span {
    node.span
}
