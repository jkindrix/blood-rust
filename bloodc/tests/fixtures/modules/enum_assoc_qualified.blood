// Test cross-module associated functions on enums using fully-qualified paths.

mod enum_associated_fns;

pub fn main() -> i32 {
    // Test associated function that returns enum type (static method)
    let c: enum_associated_fns::Color = enum_associated_fns::Color::default_color();

    // Test associated function with parameter
    let v: enum_associated_fns::Value = enum_associated_fns::Value::number(42);

    // Test another associated function
    let z: enum_associated_fns::Value = enum_associated_fns::Value::zero();

    // Also test that enum variants still work with qualified paths
    let c2: enum_associated_fns::Color = enum_associated_fns::Color::Green;
    let v2: enum_associated_fns::Value = enum_associated_fns::Value::Text;

    // Return success indicator
    0
}
