// Module containing enums with associated functions.

pub enum Color {
    Red,
    Green,
    Blue,
}

impl Color {
    pub fn default_color() -> Color {
        Color::Red
    }

    pub fn is_primary(self) -> bool {
        true
    }
}

pub enum Value {
    Number(i32),
    Text,
}

impl Value {
    pub fn number(n: i32) -> Value {
        Value::Number(n)
    }

    pub fn zero() -> Value {
        Value::Number(0)
    }
}
