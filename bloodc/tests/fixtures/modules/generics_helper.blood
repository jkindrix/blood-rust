// Module resolution test fixture: generic types.

pub struct Container<T> {
    pub value: T,
}

pub enum Either<L, R> {
    Left(L),
    Right(R),
}

pub type Pair<T> = (T, T);

pub type EitherInt<T> = Either<T, i32>;

pub fn wrap<T>(value: T) -> Container<T> {
    Container { value }
}

pub fn make_pair<T>(a: T, b: T) -> Pair<T> {
    (a, b)
}

impl<T> Container<T> {
    pub fn new(value: T) -> Container<T> {
        Container { value }
    }
}
