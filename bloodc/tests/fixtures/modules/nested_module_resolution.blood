// Module resolution test: nested modules (outer::inner::Type pattern).
mod outer;

// Test nested module type in type annotation
fn test_nested_struct() -> outer::inner::InnerData {
    outer::inner::InnerData { x: 10, y: 20 }
}

// NOTE: Nested module function calls (outer::inner::inner_fn()) require
// 3-segment path resolution in expression context, which treats it as
// Type::method rather than module::submodule::function. Not yet supported.

// Test outer module type (not nested)
fn test_outer_struct() -> outer::OuterData {
    outer::OuterData { value: 100 }
}

fn main() {
    let _ = test_nested_struct();
    let _ = test_outer_struct();
}
