// Library module that re-exports items from a submodule.
// This tests the `pub use` feature.

mod reexport_types;

// Re-export types from submodule at the library level
pub use reexport_types.Point;
pub use reexport_types.Color;
pub use reexport_types.origin;

// The library can also define its own items
pub fn create_point(x: i32, y: i32) -> Point {
    Point { x: x, y: y }
}
