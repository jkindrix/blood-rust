// Main file that imports from a library that uses pub use re-exports.
// This tests that pub use re-exports are accessible from external modules.

mod reexport_lib;

// Import from the library module (which re-exports from reexport_types)
use reexport_lib.Point;
use reexport_lib.Color;
use reexport_lib.origin;
use reexport_lib.create_point;

pub fn main() -> i32 {
    // Use the re-exported type
    let p: Point = origin();

    // Use the re-exported function wrapper
    let q: Point = create_point(10, 20);

    // Use the re-exported enum
    let c: Color = Color::Red;

    // Return success
    p.x + q.y
}
