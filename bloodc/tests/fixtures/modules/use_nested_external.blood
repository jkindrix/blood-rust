// Test use statements for importing from nested external modules.

mod outer;

// Import from nested module using dot-separated path
use outer.inner.InnerData;
use outer.inner.inner_fn;

// Import from outer module
use outer.OuterData;

pub fn main() -> i32 {
    // Use imported types from nested module
    let inner: InnerData = InnerData { x: 10, y: 20 };

    // Use imported function from nested module
    let v: i32 = inner_fn();

    // Use imported type from outer module
    let outer: OuterData = OuterData { value: 100 };

    inner.x + v + outer.value
}
