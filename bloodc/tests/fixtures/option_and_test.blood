// Test Option.and() method

fn main() {
    // Test case 1: Some.and(Some) -> Some
    let a: Option<i32> = Some(5);
    let b: Option<i64> = Some(10i64);
    let result = a.and(b);

    if result.is_some() {
        println("and(Some, Some) = Some - PASS");
    } else {
        println("and(Some, Some) = None - FAIL");
    }

    // Test case 2: None.and(Some) -> None
    let c: Option<i32> = None;
    let d: Option<i64> = Some(20i64);
    let result2 = c.and(d);

    if result2.is_none() {
        println("and(None, Some) = None - PASS");
    } else {
        println("and(None, Some) = Some - FAIL");
    }

    // Test case 3: Some.and(None) -> None
    let e: Option<i32> = Some(5);
    let f: Option<i64> = None;
    let result3 = e.and(f);

    if result3.is_none() {
        println("and(Some, None) = None - PASS");
    } else {
        println("and(Some, None) = Some - FAIL");
    }
}
