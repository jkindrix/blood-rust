// Test Result methods

fn main() {
    let ok_val: Result<i32, i32> = Ok(42);

    if ok_val.is_ok() {
        println!("Result is Ok");
    }

    if !ok_val.is_err() {
        println!("Result is not Err");
    }

    let val = ok_val.unwrap();
    println!("Unwrapped Ok value");

    let err_val: Result<i32, i32> = Err(99);

    if err_val.is_err() {
        println!("Result is Err");
    }

    let err = err_val.unwrap_err();
    println!("Unwrapped Err value");

    println!("Done");
}
