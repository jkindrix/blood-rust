fn main() -> i32 {
    // Less than
    if 3 < 5 { println_int(1); } else { println_int(0); }

    // Greater than
    if 10 > 2 { println_int(1); } else { println_int(0); }

    // Less than or equal (equal case)
    if 5 <= 5 { println_int(1); } else { println_int(0); }

    // Greater than or equal (greater case)
    if 7 >= 3 { println_int(1); } else { println_int(0); }

    // Equal
    if 42 == 42 { println_int(1); } else { println_int(0); }

    // Not equal
    if 1 != 2 { println_int(1); } else { println_int(0); }

    // False cases for verification via exit code
    if 5 < 3 { return 1; }
    if 2 > 10 { return 2; }
    if 6 <= 5 { return 3; }
    if 3 >= 7 { return 4; }
    if 1 == 2 { return 5; }
    if 5 != 5 { return 6; }
