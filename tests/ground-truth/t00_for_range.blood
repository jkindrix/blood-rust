// Test: for loop over integer range
// EXPECT: 55
// EXPECT: 45
// EXPECT: 10
fn main() -> i32 {
    // Sum 1..=10 using for loop over range
    let mut sum: i32 = 0;
    for i in 1..11 {
        sum = sum + i;
    }
    println_int(sum); // 55
    if sum != 55 { return 1; }

    // for loop starting from non-zero
    let mut sum2: i32 = 0;
    for i in 1..10 {
        sum2 = sum2 + i;
    }
    println_int(sum2); // 1+2+...+9 = 45
    if sum2 != 45 { return 2; }

    // for loop with small range
    let mut count: i32 = 0;
    for i in 0..10 {
        count = count + 1;
    }
    println_int(count); // 10
    if count != 10 { return 3; }

    0
}
