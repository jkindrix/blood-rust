// Test: function calls, multiple params, return values
// EXPECT: 30
// EXPECT: 200
// EXPECT: 13
fn add(a: i32, b: i32) -> i32 {
    a + b
}

fn multiply(a: i32, b: i32) -> i32 {
    a * b
}

fn fibonacci(n: i32) -> i32 {
    if n <= 1 {
        n
    } else {
        let mut a: i32 = 0;
        let mut b: i32 = 1;
        let mut i: i32 = 2;
        while i <= n {
            let temp: i32 = a + b;
            a = b;
            b = temp;
            i = i + 1;
        }
        b
    }
}

