// Test: let bindings, mutability, variable shadowing
// EXPECT: 10
// EXPECT: 25
// EXPECT: 100
fn main() -> i32 {
    // Immutable binding
    let x: i32 = 10;
    println_int(x);

    // Mutable binding
    let mut y: i32 = 20;
    y = y + 5;
    println_int(y);

    // Variable shadowing (new let with same name)
    let z: i32 = 50;
    let z: i32 = z * 2;
    println_int(z);

    if x != 10 { return 1; }
    if y != 25 { return 2; }
    if z != 100 { return 3; }
    0
}
