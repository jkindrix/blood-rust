fn classify_char(c: i32) -> i32 {
    // 0 = digit, 1 = uppercase, 2 = lowercase, 3 = other
    if c >= 48 && c <= 57 {
        0
    } else if c >= 65 && c <= 90 {
        1
    } else if c >= 97 && c <= 122 {
        2
    } else {
        3
    }
}

fn grade(score: i32) -> i32 {
    // Nested ifs for letter grade: A=4, B=3, C=2, D=1, F=0
    if score >= 90 {
        4
    } else {
        if score >= 80 {
            3
        } else {
            if score >= 70 {
                2
            } else {
                if score >= 60 {
                    1
                } else {
                    0
                }
            }
        }
    }
}

