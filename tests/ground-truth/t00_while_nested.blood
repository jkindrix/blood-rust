// Test: nested while loops
// EXPECT: 45
// EXPECT: 20
fn main() -> i32 {
    // Triangular number T(9): count iterations in nested loop
    let mut tri: i32 = 0;
    let mut r: i32 = 1;
    while r <= 9 {
        let mut c: i32 = 1;
        while c <= r {
            tri = tri + 1;
            c = c + 1;
        }
        r = r + 1;
    }
    println_int(tri); // 1+2+3+4+5+6+7+8+9 = 45

    // Count pairs (i,j) where i < j and i+j <= 10, for i,j in 1..9
    let mut count: i32 = 0;
    let mut a: i32 = 1;
    while a <= 9 {
        let mut b: i32 = a + 1;
        while b <= 9 {
            if a + b <= 10 {
                count = count + 1;
            }
            b = b + 1;
        }
        a = a + 1;
    }
    println_int(count); // 8+6+4+2 = 20

    if tri != 45 { return 1; }
    if count != 20 { return 2; }
    0
}
