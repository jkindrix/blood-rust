// Test: basic enum declaration and matching
// EXPECT: 3
// EXPECT: 25
// EXPECT: 0
enum Shape {
    Circle(i32),
    Square(i32),
    Empty,
}

fn area(s: Shape) -> i32 {
    match s {
        Shape::Circle(r) => r * r * 3,
        Shape::Square(side) => side * side,
        Shape::Empty => 0,
    }
}

fn main() -> i32 {
    let c: Shape = Shape::Circle(1);
    let a1: i32 = area(c);
    println_int(a1);

    let sq: Shape = Shape::Square(5);
    let a2: i32 = area(sq);
    println_int(a2);

    let e: Shape = Shape::Empty;
    let a3: i32 = area(e);
    println_int(a3);

    if a1 != 3 { return 1; }
    if a2 != 25 { return 2; }
    if a3 != 0 { return 3; }
    0
}
