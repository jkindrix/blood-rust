// Test: enum with all-unit variants (no payloads)
// EXPECT: 1
// EXPECT: 2
// EXPECT: 3
// EXPECT: 4
enum Direction {
    North,
    South,
    East,
    West,
}

fn direction_to_int(d: Direction) -> i32 {
    match d {
        Direction::North => 1,
        Direction::South => 2,
        Direction::East => 3,
        Direction::West => 4,
    }
}

fn opposite(d: Direction) -> Direction {
    match d {
        Direction::North => Direction::South,
        Direction::South => Direction::North,
        Direction::East => Direction::West,
        Direction::West => Direction::East,
    }
}

fn main() -> i32 {
    println_int(direction_to_int(Direction::North));
    println_int(direction_to_int(Direction::South));
    println_int(direction_to_int(Direction::East));
    println_int(direction_to_int(Direction::West));

    let opp: Direction = opposite(Direction::North);
    let opp_val: i32 = direction_to_int(opp);
    if opp_val != 2 { return 1; }

    let opp2: Direction = opposite(Direction::East);
    let opp2_val: i32 = direction_to_int(opp2);
    if opp2_val != 4 { return 2; }

    0
}
