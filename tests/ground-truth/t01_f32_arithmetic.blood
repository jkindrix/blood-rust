// Test: f32 floating-point arithmetic
// EXPECT: 10
// EXPECT: 6
fn main() -> i32 {
    // f32 addition
    let a: f32 = 3.0f32;
    let b: f32 = 7.0f32;
    let sum: f32 = a + b;
    let sum_i: i32 = sum as i32;
    println_int(sum_i); // 10
    if sum_i != 10 { return 1; }

    // f32 multiplication
    let c: f32 = 2.0f32;
    let d: f32 = 3.0f32;
    let prod: f32 = c * d;
    let prod_i: i32 = prod as i32;
    println_int(prod_i); // 6
    if prod_i != 6 { return 2; }

    // f32 comparison
    let x: f32 = 1.5f32;
    let y: f32 = 2.5f32;
    if x > y { return 3; }
    if x == y { return 4; }

    0
}
