// Test: f64 floating-point arithmetic
// EXPECT: 7
// EXPECT: 2
// EXPECT: 12
// EXPECT: 3
fn main() -> i32 {
    // Addition
    let a: f64 = 3.0;
    let b: f64 = 4.0;
    let sum: f64 = a + b;
    let sum_i: i64 = sum as i64;
    println_i64(sum_i); // 7
    if sum_i != 7 { return 1; }

    // Subtraction
    let diff: f64 = 5.5 - 3.5;
    let diff_i: i64 = diff as i64;
    println_i64(diff_i); // 2
    if diff_i != 2 { return 2; }

    // Multiplication
    let prod: f64 = 3.0 * 4.0;
    let prod_i: i64 = prod as i64;
    println_i64(prod_i); // 12
    if prod_i != 12 { return 3; }

    // Division
    let quot: f64 = 15.0 / 5.0;
    let quot_i: i64 = quot as i64;
    println_i64(quot_i); // 3
    if quot_i != 3 { return 4; }

    // Comparison
    let x: f64 = 1.5;
    let y: f64 = 2.5;
    if x > y { return 5; }
    if x >= y { return 6; }
    if y < x { return 7; }
    if y <= x { return 8; }
    if x == y { return 9; }

    0
}
