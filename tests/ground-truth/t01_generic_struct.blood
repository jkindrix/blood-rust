// Test: generic struct with type parameter
// EXPECT: 42
// EXPECT: 99
struct Wrapper<T> {
    value: T,
}

fn unwrap_int(w: Wrapper<i32>) -> i32 {
    w.value
}

fn main() -> i32 {
    let w1: Wrapper<i32> = Wrapper { value: 42 };
    let v1: i32 = unwrap_int(w1);
    println_int(v1);

    let w2: Wrapper<i32> = Wrapper { value: 99 };
    let v2: i32 = w2.value;
    println_int(v2);

    if v1 != 42 { return 1; }
    if v2 != 99 { return 2; }
    0
}
