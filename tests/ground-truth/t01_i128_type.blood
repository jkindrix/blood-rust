// Test: i128 integer type
// EXPECT: 100
// EXPECT: -50
fn main() -> i32 {
    // Basic i128
    let a: i128 = 100;
    let a_i: i64 = a as i64;
    println_i64(a_i); // 100
    if a_i != 100 { return 1; }

    // i128 arithmetic
    let b: i128 = 75;
    let c: i128 = 125;
    let diff: i128 = b - c;
    let diff_i: i64 = diff as i64;
    println_i64(diff_i); // -50
    if diff_i != -50 { return 2; }

    0
}
