// Test: match expression on integers
// EXPECT: 100
// EXPECT: 200
// EXPECT: 999
fn classify(x: i32) -> i32 {
    match x {
        0 => 100,
        1 => 200,
        _ => 999,
    }
}

fn main() -> i32 {
    let a: i32 = classify(0);
    println_int(a);

    let b: i32 = classify(1);
    println_int(b);

    let c: i32 = classify(42);
    println_int(c);

    if a != 100 { return 1; }
    if b != 200 { return 2; }
    if c != 999 { return 3; }
    0
}
