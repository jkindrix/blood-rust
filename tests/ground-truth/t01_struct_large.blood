// Test: large struct with 10 fields (ABI/layout correctness)
// EXPECT: 55
struct TenFields {
    f1: i32,
    f2: i32,
    f3: i32,
    f4: i32,
    f5: i32,
    f6: i32,
    f7: i32,
    f8: i32,
    f9: i32,
    f10: i32,
}

fn sum_all(s: TenFields) -> i32 {
    s.f1 + s.f2 + s.f3 + s.f4 + s.f5 + s.f6 + s.f7 + s.f8 + s.f9 + s.f10
}

fn main() -> i32 {
    let s: TenFields = TenFields {
        f1: 1, f2: 2, f3: 3, f4: 4, f5: 5,
        f6: 6, f7: 7, f8: 8, f9: 9, f10: 10,
    };
    let total: i32 = sum_all(s);
    println_int(total); // 55

    // Verify individual field access
    let s2: TenFields = TenFields {
        f1: 1, f2: 2, f3: 3, f4: 4, f5: 5,
        f6: 6, f7: 7, f8: 8, f9: 9, f10: 10,
    };
    if s2.f1 != 1 { return 1; }
    if s2.f5 != 5 { return 5; }
    if s2.f10 != 10 { return 10; }
    if total != 55 { return 11; }
    0
}
