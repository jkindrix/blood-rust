// Test: struct with many fields
// EXPECT: 150
struct Record {
    a: i32,
    b: i32,
    c: i32,
    d: i32,
    e: i32,
}

fn sum_record(r: Record) -> i32 {
    r.a + r.b + r.c + r.d + r.e
}

fn main() -> i32 {
    let r: Record = Record { a: 10, b: 20, c: 30, d: 40, e: 50 };
    let s: i32 = sum_record(r);
    println_int(s);

    if s != 150 { return 1; }
    0
}
