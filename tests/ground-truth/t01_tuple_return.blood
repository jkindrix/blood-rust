// Test: functions returning tuples
// EXPECT: 3
// EXPECT: 1
fn divmod(a: i32, b: i32) -> (i32, i32) {
    (a / b, a % b)
}

fn main() -> i32 {
    let result: (i32, i32) = divmod(10, 3);
    match result {
        (q, r) => {
            println_int(q);
            println_int(r);
            if q != 3 { return 1; }
            // 10 % 3 = 1
            if r != 1 { return 2; }
            0
        },
    }
}
