// Test: unsigned integer types (u8, u16, u32, u64)
// EXPECT: 255
// EXPECT: 65535
// EXPECT: 1000000
// EXPECT: 42
fn main() -> i32 {
    // u8 max value
    let a: u8 = 255;
    let a_i: i32 = a as i32;
    println_int(a_i); // 255
    if a_i != 255 { return 1; }

    // u16 max value
    let b: u16 = 65535;
    let b_i: i32 = b as i32;
    println_int(b_i); // 65535
    if b_i != 65535 { return 2; }

    // u32 arithmetic
    let c: u32 = 999990;
    let d: u32 = 10;
    let sum: u32 = c + d;
    let sum_i: i64 = sum as i64;
    println_i64(sum_i); // 1000000
    if sum_i != 1000000 { return 3; }

    // u64 basic
    let e: u64 = 42;
    let e_i: i64 = e as i64;
    println_i64(e_i); // 42
    if e_i != 42 { return 4; }

    // Unsigned comparison
    let x: u32 = 10;
    let y: u32 = 20;
    if x > y { return 5; }
    if x == y { return 6; }

    0
}
