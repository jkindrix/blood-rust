// Test: region blocks with conditional logic inside
// EXPECT: 100
// EXPECT: -1
fn compute_in_region(x: i32) -> i32 {
    region {
        if x > 0 {
            x * x
        } else {
            -1
        }
    }
}

fn main() -> i32 {
    let a: i32 = compute_in_region(10);
    let b: i32 = compute_in_region(-5);

    println_int(a);
    println_int(b);

    if a != 100 { return 1; }
    if b != -1 { return 2; }
    0
}
