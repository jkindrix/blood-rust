// Test: pure function annotation
// EXPECT: 30
// EXPECT: 200
fn add(a: i32, b: i32) -> i32 / pure {
    a + b
}

fn multiply(a: i32, b: i32) -> i32 / pure {
    a * b
}

fn main() -> i32 {
    let s: i32 = add(10, 20);
    println_int(s);

    let p: i32 = multiply(10, 20);
    println_int(p);

    if s != 30 { return 1; }
    if p != 200 { return 2; }
    0
}
