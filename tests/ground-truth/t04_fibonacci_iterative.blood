// Test: iterative fibonacci (while loop, mutation, no recursion)
// EXPECT: 0
// EXPECT: 1
// EXPECT: 1
// EXPECT: 55
// EXPECT: 832040
fn fib(n: i32) -> i32 {
    if n <= 0 { return 0; }
    if n == 1 { return 1; }
    let mut a: i32 = 0;
    let mut b: i32 = 1;
    let mut i: i32 = 2;
    while i <= n {
        let temp: i32 = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    b
}

fn main() -> i32 {
    println_int(fib(0));  // 0
    println_int(fib(1));  // 1
    println_int(fib(2));  // 1
    println_int(fib(10)); // 55
    println_int(fib(30)); // 832040

    if fib(0) != 0 { return 1; }
    if fib(1) != 1 { return 2; }
    if fib(2) != 1 { return 3; }
    if fib(10) != 55 { return 4; }
    if fib(30) != 832040 { return 5; }
    0
}
