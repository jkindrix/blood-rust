// Test: command-line argument builtins
// EXPECT: 1
fn main() -> i32 {
    // When run without arguments, args_count should return 1 (program name only)
    let count: i32 = args_count();
    // At minimum the program itself is an argument
    if count < 1 { return 1; }
    println_int(count); // 1

    // Get the program name (first argument)
    let prog: &str = args_get(0);
    let len: i64 = str_len(prog);
    // Program name should be non-empty
    if len < 1 { return 2; }

    0
}
