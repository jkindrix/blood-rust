// Test: array operations - sum and product via destructuring
// EXPECT: 15
// EXPECT: 120
fn array_sum(arr: [i32; 5]) -> i32 {
    match arr {
        [a, b, c, d, e] => a + b + c + d + e,
    }
}

fn array_product(arr: [i32; 5]) -> i32 {
    match arr {
        [a, b, c, d, e] => a * b * c * d * e,
    }
}

fn main() -> i32 {
    let data: [i32; 5] = [1, 2, 3, 4, 5];
    let s: i32 = array_sum(data);
    println_int(s); // 15

    let p: i32 = array_product(data);
    println_int(p); // 120

    if s != 15 { return 1; }
    if p != 120 { return 2; }
    0
}
