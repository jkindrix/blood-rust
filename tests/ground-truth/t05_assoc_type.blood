// Test: trait with associated type
trait Container {
    type Item;
    fn get(self) -> Self::Item;
}

struct IntBox {
    val: i32,
}

impl Container for IntBox {
    type Item = i32;
    fn get(self) -> i32 { self.val }
}

fn main() -> i32 {
    let b: IntBox = IntBox { val: 42 };
    println_int(b.get());
    0
}
