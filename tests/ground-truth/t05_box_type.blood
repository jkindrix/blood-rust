// Test: Box<T> heap allocation
// EXPECT: 42
// EXPECT: 100
fn main() -> i32 {
    // Basic Box
    let b: Box<i32> = Box::new(42);
    let val: i32 = *b;
    println_int(val); // 42
    if val != 42 { return 1; }

    // Box with larger value
    let b2: Box<i32> = Box::new(100);
    println_int(*b2); // 100
    if *b2 != 100 { return 2; }

    0
}
