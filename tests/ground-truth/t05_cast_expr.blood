// Test: cast expressions using 'as' keyword
// EXPECT: 42
// EXPECT: 1000000
// EXPECT: 3
// EXPECT: 65
fn main() -> i32 {
    // i32 to i64
    let a: i32 = 42;
    let b: i64 = a as i64;
    println_i64(b); // 42
    if b != 42 { return 1; }

    // i64 to i32 (truncation within range)
    let c: i64 = 1000000;
    let d: i32 = c as i32;
    println_int(d); // 1000000
    if d != 1000000 { return 2; }

    // f64 to i64 (truncation)
    let e: f64 = 3.7;
    let f: i64 = e as i64;
    println_i64(f); // 3
    if f != 3 { return 3; }

    // i32 to f64 to i32
    let g: i32 = 65;
    let h: f64 = g as f64;
    let i: i32 = h as i32;
    println_int(i); // 65
    if i != 65 { return 4; }

    0
}
