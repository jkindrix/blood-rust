// Test: char_to_string conversion builtin
// EXPECT: A
// EXPECT: 1
fn main() -> i32 {
    let ch: char = 'A';
    let s: &str = char_to_string(ch);
    println_str(s); // A
    if !str_eq(s, "A") { return 1; }

    let s_len: i64 = str_len(s);
    println_i64(s_len); // 1
    if s_len != 1 { return 2; }

    0
}
