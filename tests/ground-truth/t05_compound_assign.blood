// Test: compound assignment operators (+=, -=, *=, /=, %=)
// EXPECT: 15
// EXPECT: 10
// EXPECT: 30
// EXPECT: 6
// EXPECT: 1
fn main() -> i32 {
    // +=
    let mut a: i32 = 10;
    a += 5;
    println_int(a); // 15
    if a != 15 { return 1; }

    // -=
    let mut b: i32 = 15;
    b -= 5;
    println_int(b); // 10
    if b != 10 { return 2; }

    // *=
    let mut c: i32 = 6;
    c *= 5;
    println_int(c); // 30
    if c != 30 { return 3; }

    // /=
    let mut d: i32 = 30;
    d /= 5;
    println_int(d); // 6
    if d != 6 { return 4; }

    // %=
    let mut e: i32 = 10;
    e %= 3;
    println_int(e); // 1
    if e != 1 { return 5; }

    0
}
