// Test: const generic parameters
// EXPECT: 15
// EXPECT: 6
fn sum_array<const N: usize>(arr: [i32; N]) -> i32 {
    let mut sum: i32 = 0;
    let mut i: i32 = 0;
    while i < N as i32 {
        sum = sum + arr[i];
        i = i + 1;
    }
    sum
}

fn main() -> i32 {
    let a: [i32; 5] = [1, 2, 3, 4, 5];
    let s: i32 = sum_array(a);
    println_int(s); // 15
    if s != 15 { return 1; }

    let b: [i32; 3] = [1, 2, 3];
    let s2: i32 = sum_array(b);
    println_int(s2); // 6
    if s2 != 6 { return 2; }

    0
}
