// Test: f64_to_string and f32_to_string conversion builtins
// EXPECT: 4
// EXPECT: 3
fn main() -> i32 {
    // f64 to string: "3.14" is 4 characters
    let s: &str = f64_to_string(3.14);
    let len: i64 = str_len(s);
    println_i64(len); // 4
    if len != 4 { return 1; }

    // f32 to string: "2.5" is 3 characters
    let s2: &str = f32_to_string(2.5f32);
    let len2: i64 = str_len(s2);
    println_i64(len2); // 3
    if len2 != 3 { return 2; }

    0
}
