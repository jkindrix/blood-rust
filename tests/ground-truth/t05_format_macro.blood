// Test: format! macro for string formatting
// EXPECT: hello world
// EXPECT: x = 42
fn main() -> i32 {
    // Basic format
    let s: &str = format!("hello {}", "world");
    println_str(s); // hello world

    // Format with integer
    let x: i32 = 42;
    let s2: &str = format!("x = {}", x);
    println_str(s2); // x = 42

    0
}
