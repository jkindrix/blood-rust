// Test: generic function called with different types (monomorphization)
// EXPECT: 42
// EXPECT: 1
fn identity<T>(x: T) -> T {
    x
}

fn main() -> i32 {
    // Call identity with i32
    let a: i32 = identity(42);
    println_int(a);

    // Call identity with bool
    let b: bool = identity(true);
    if b {
        println_int(1);
    } else {
        println_int(0);
    }

    if a != 42 { return 1; }
    if !b { return 2; }
    0
}
