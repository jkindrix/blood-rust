// Test: i128_to_string conversion builtin
// EXPECT: 1000000
// EXPECT: -1
fn main() -> i32 {
    let s1: &str = i128_to_string(1000000);
    println_str(s1); // 1000000
    if !str_eq(s1, "1000000") { return 1; }

    let s2: &str = i128_to_string(-1);
    println_str(s2); // -1
    if !str_eq(s2, "-1") { return 2; }

    0
}
