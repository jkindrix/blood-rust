// Test: i32_to_i64 and i64_to_i32 conversion builtins
// EXPECT: 42
// EXPECT: 100
fn main() -> i32 {
    // i32 to i64
    let a: i32 = 42;
    let b: i64 = i32_to_i64(a);
    println_i64(b); // 42
    if b != 42 { return 1; }

    // i64 to i32
    let c: i64 = 100;
    let d: i32 = i64_to_i32(c);
    println_int(d); // 100
    if d != 100 { return 2; }

    // Round-trip
    let e: i32 = 77;
    let f: i32 = i64_to_i32(i32_to_i64(e));
    if f != 77 { return 3; }

    0
}
