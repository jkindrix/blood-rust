// Test: 64-bit integer arithmetic
// EXPECT: 3000000000
// EXPECT: 9000000000000000000
// EXPECT: 1500000000
fn main() -> i32 {
    // Values exceeding i32 range
    let a: i64 = 1000000000;
    let b: i64 = 2000000000;
    let sum: i64 = a + b;
    println_i64(sum); // 3000000000 (exceeds i32 max of 2147483647)

    // Large multiplication
    let c: i64 = 3000000000;
    let d: i64 = c * c;
    println_i64(d); // 9000000000000000000

    // Division
    let e: i64 = d / 6000000000;
    println_i64(e); // 1500000000

    if sum != 3000000000 { return 1; }
    if d != 9000000000000000000 { return 2; }
    if e != 1500000000 { return 3; }
    0
}
