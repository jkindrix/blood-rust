// Test: i8_to_string and i16_to_string conversion builtins
// EXPECT: 127
// EXPECT: -128
// EXPECT: 32767
fn main() -> i32 {
    let s1: &str = i8_to_string(127);
    println_str(s1); // 127
    if !str_eq(s1, "127") { return 1; }

    let s2: &str = i8_to_string(-128);
    println_str(s2); // -128
    if !str_eq(s2, "-128") { return 2; }

    let s3: &str = i16_to_string(32767);
    println_str(s3); // 32767
    if !str_eq(s3, "32767") { return 3; }

    0
}
