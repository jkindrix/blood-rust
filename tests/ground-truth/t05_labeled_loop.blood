// Test: labeled loops with break and continue
// EXPECT: 42
fn main() -> i32 {
    // Labeled outer loop, break from inner
    let mut result: i32 = 0;
    let mut i: i32 = 0;
    'outer: while i < 10 {
        let mut j: i32 = 0;
        while j < 10 {
            if i * 10 + j == 42 {
                result = i * 10 + j;
                break 'outer;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    println_int(result); // 42
    if result != 42 { return 1; }

    0
}
