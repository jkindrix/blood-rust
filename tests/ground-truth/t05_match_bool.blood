// Test: matching on bool values
// EXPECT: 1
// EXPECT: 0
fn bool_to_int(b: bool) -> i32 {
    match b {
        true => 1,
        false => 0,
    }
}

fn main() -> i32 {
    println_int(bool_to_int(true));
    println_int(bool_to_int(false));

    if bool_to_int(true) != 1 { return 1; }
    if bool_to_int(false) != 0 { return 2; }
    0
}
