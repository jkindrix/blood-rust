// Test: nested match expressions
// EXPECT: 100
// EXPECT: 0
// EXPECT: 42
fn classify(x: i32, y: i32) -> i32 {
    match x {
        0 => match y {
            0 => 0,
            _ => 100,
        },
        _ => 42,
    }
}

fn main() -> i32 {
    let a: i32 = classify(0, 5);
    println_int(a);

    let b: i32 = classify(0, 0);
    println_int(b);

    let c: i32 = classify(7, 0);
    println_int(c);

    if a != 100 { return 1; }
    if b != 0 { return 2; }
    if c != 42 { return 3; }
    0
}
