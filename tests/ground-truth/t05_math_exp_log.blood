// Test: exp, log, pow math builtins
// EXPECT: 1
// EXPECT: 0
// EXPECT: 8
fn main() -> i32 {
    // exp(0.0) = 1.0
    let e: f64 = exp(0.0);
    let e_i: i64 = e as i64;
    println_i64(e_i); // 1
    if e_i != 1 { return 1; }

    // log(1.0) = 0.0 (natural log)
    let l: f64 = log(1.0);
    let l_i: i64 = l as i64;
    println_i64(l_i); // 0
    if l_i != 0 { return 2; }

    // pow(2.0, 3.0) = 8.0
    let p: f64 = pow(2.0, 3.0);
    let p_i: i64 = p as i64;
    println_i64(p_i); // 8
    if p_i != 8 { return 3; }

    0
}
