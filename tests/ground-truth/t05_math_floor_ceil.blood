// Test: floor and ceil math builtins
// EXPECT: 3
// EXPECT: 4
// EXPECT: -4
// EXPECT: -3
fn main() -> i32 {
    // floor(3.7) = 3.0
    let f: f64 = floor(3.7);
    let f_i: i64 = f as i64;
    println_i64(f_i); // 3
    if f_i != 3 { return 1; }

    // ceil(3.2) = 4.0
    let c: f64 = ceil(3.2);
    let c_i: i64 = c as i64;
    println_i64(c_i); // 4
    if c_i != 4 { return 2; }

    // floor(-3.2) = -4.0
    let f2: f64 = floor(-3.2);
    let f2_i: i64 = f2 as i64;
    println_i64(f2_i); // -4
    if f2_i != -4 { return 3; }

    // ceil(-3.7) = -3.0
    let c2: f64 = ceil(-3.7);
    let c2_i: i64 = c2 as i64;
    println_i64(c2_i); // -3
    if c2_i != -3 { return 4; }

    0
}
