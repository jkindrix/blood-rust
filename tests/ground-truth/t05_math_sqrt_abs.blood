// Test: sqrt and abs math builtins
// EXPECT: 3
// EXPECT: 5
// EXPECT: 4
fn main() -> i32 {
    // sqrt(9.0) = 3.0
    let s: f64 = sqrt(9.0);
    let s_i: i64 = s as i64;
    println_i64(s_i); // 3
    if s_i != 3 { return 1; }

    // sqrt(25.0) = 5.0
    let s2: f64 = sqrt(25.0);
    let s2_i: i64 = s2 as i64;
    println_i64(s2_i); // 5
    if s2_i != 5 { return 2; }

    // abs(-4.0) = 4.0
    let a: f64 = abs(-4.0);
    let a_i: i64 = a as i64;
    println_i64(a_i); // 4
    if a_i != 4 { return 3; }

    // abs of positive is identity
    let b: f64 = abs(7.0);
    let b_i: i64 = b as i64;
    if b_i != 7 { return 4; }

    0
}
