// Test: trigonometric builtins (sin, cos)
// EXPECT: 0
// EXPECT: 1
fn main() -> i32 {
    // sin(0.0) = 0.0
    let s: f64 = sin(0.0);
    let s_i: i64 = s as i64;
    println_i64(s_i); // 0
    if s_i != 0 { return 1; }

    // cos(0.0) = 1.0
    let c: f64 = cos(0.0);
    let c_i: i64 = c as i64;
    println_i64(c_i); // 1
    if c_i != 1 { return 2; }

    // sin and cos are bounded: -1 <= sin(x) <= 1
    let s2: f64 = sin(1.0);
    if s2 < -1.0 { return 3; }
    if s2 > 1.0 { return 4; }

    0
}
