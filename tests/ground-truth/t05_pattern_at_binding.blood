// Test: pattern with @ binding in match
// EXPECT: 42
enum Opt {
    Some(i32),
    None,
}

fn main() -> i32 {
    let x: Opt = Opt::Some(42);
    match x {
        Opt::Some(val @ 42) => println_int(val),
        Opt::Some(val) => println_int(val),
        Opt::None => println_int(0),
    }
    0
}
