// Test: range patterns in match expressions
// EXPECT: 1
// EXPECT: 2
// EXPECT: 3
fn classify_digit(n: i32) -> i32 {
    match n {
        0..=3 => 1,     // low
        4..=6 => 2,     // mid
        7..=9 => 3,     // high
        _ => 0,         // out of range
    }
}

fn main() -> i32 {
    let a: i32 = classify_digit(2);
    println_int(a); // 1 (low)
    if a != 1 { return 1; }

    let b: i32 = classify_digit(5);
    println_int(b); // 2 (mid)
    if b != 2 { return 2; }

    let c: i32 = classify_digit(8);
    println_int(c); // 3 (high)
    if c != 3 { return 3; }

    // Out of range
    let d: i32 = classify_digit(15);
    if d != 0 { return 4; }

    0
}
