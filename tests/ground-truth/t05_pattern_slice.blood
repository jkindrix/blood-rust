// Test: slice patterns in match
// EXPECT: 1
// EXPECT: 3
fn main() -> i32 {
    let arr: [i32; 3] = [1, 2, 3];

    // Match on array with binding
    let first: i32 = match arr {
        [a, _, _] => a,
    };
    println_int(first); // 1
    if first != 1 { return 1; }

    let last: i32 = match arr {
        [_, _, c] => c,
    };
    println_int(last); // 3
    if last != 3 { return 2; }

    0
}
