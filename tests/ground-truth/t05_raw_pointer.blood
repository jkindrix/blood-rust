// Test: raw pointer types (*const T, *mut T)
// EXPECT: 42
fn main() -> i32 {
    let x: i32 = 42;

    // Create a raw const pointer
    let p: *const i32 = &x as *const i32;

    // Dereference raw pointer in unsafe block
    let val: i32 = @unsafe { *p };
    println_int(val); // 42
    if val != 42 { return 1; }

    0
}
