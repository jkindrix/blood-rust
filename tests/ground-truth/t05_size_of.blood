// Test: size_of builtins for type sizes
// EXPECT: 4
// EXPECT: 8
// EXPECT: 1
fn main() -> i32 {
    let si32: i64 = size_of_i32();
    println_i64(si32); // 4
    if si32 != 4 { return 1; }

    let si64: i64 = size_of_i64();
    println_i64(si64); // 8
    if si64 != 8 { return 2; }

    let sbool: i64 = size_of_bool();
    println_i64(sbool); // 1
    if sbool != 1 { return 3; }

    0
}
