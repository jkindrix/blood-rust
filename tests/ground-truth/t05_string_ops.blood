// Test: string operations (str_concat, str_len, str_eq)
// EXPECT: hello world
// EXPECT: 11
// EXPECT: 1
// EXPECT: 0
fn main() -> i32 {
    let a: &str = "hello ";
    let b: &str = "world";
    let c: &str = str_concat(a, b);
    println_str(c);

    let len: i64 = str_len(c);
    println_i64(len); // 11

    // Equality
    let eq1: bool = str_eq("abc", "abc");
    if eq1 { println_int(1); } else { println_int(0); }

    let eq2: bool = str_eq("abc", "xyz");
    if eq2 { println_int(1); } else { println_int(0); }

    if len != 11 { return 1; }
    if !eq1 { return 2; }
    if eq2 { return 3; }
    0
}
