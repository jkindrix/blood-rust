// Test: struct update/spread syntax
// EXPECT: 10
// EXPECT: 99
struct Point {
    x: i32,
    y: i32,
}

fn main() -> i32 {
    let p1: Point = Point { x: 10, y: 20 };
    let p2: Point = Point { y: 99, ..p1 };
    println_int(p2.x);
    println_int(p2.y);
    if p2.x != 10 { return 1; }
    if p2.y != 99 { return 2; }
    0
}
