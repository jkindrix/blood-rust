// Test: trait method with default implementation
trait Greet {
    fn name(self) -> i32;
    fn greet(self) -> i32 {
        self.name() + 1
    }
}

struct Person {
    id: i32,
}

impl Greet for Person {
    fn name(self) -> i32 { self.id }
}

fn main() -> i32 {
    let p: Person = Person { id: 5 };
    println_int(p.greet());
    0
}
