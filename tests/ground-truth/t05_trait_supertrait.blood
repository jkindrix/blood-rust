// Test: trait inheritance with supertrait
// EXPECT: 10
// EXPECT: 11
trait Base {
    fn base_val(self) -> i32;
}

trait Sub: Base {
    fn sub_val(self) -> i32;
}

struct Foo {
    x: i32,
}

impl Base for Foo {
    fn base_val(self) -> i32 { self.x }
}

impl Sub for Foo {
    fn sub_val(self) -> i32 { self.x + 1 }
}

fn main() -> i32 {
    let f: Foo = Foo { x: 10 };
    println_int(f.base_val());
    println_int(f.sub_val());
    0
}
