// Test: turbofish syntax for explicit type arguments
// EXPECT: 42
// EXPECT: 1
fn identity<T>(x: T) -> T {
    x
}

fn main() -> i32 {
    let a: i32 = identity::<i32>(42);
    println_int(a);

    let b: bool = identity::<bool>(true);
    if b {
        println_int(1);
    } else {
        println_int(0);
    }

    if a != 42 { return 1; }
    if !b { return 2; }
    0
}
