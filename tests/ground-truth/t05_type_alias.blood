// Test: type aliases
// EXPECT: 42
// EXPECT: 7
type Int = i32;
type Pair = (i32, i32);

fn add_ints(a: Int, b: Int) -> Int {
    a + b
}

fn main() -> i32 {
    let x: Int = 20;
    let y: Int = 22;
    let sum: Int = add_ints(x, y);
    println_int(sum); // 42
    if sum != 42 { return 1; }

    let p: Pair = (3, 4);
    let (a, b): (i32, i32) = p;
    let total: i32 = a + b;
    println_int(total); // 7
    if total != 7 { return 2; }

    0
}
