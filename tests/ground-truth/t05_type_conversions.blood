// Test: type conversion builtins (int_to_string, bool_to_string, i64_to_string)
// EXPECT: 42
// EXPECT: true
// EXPECT: false
// EXPECT: 1000000000000
fn main() -> i32 {
    // int_to_string
    let s1: &str = int_to_string(42);
    println_str(s1);
    if !str_eq(s1, "42") { return 1; }

    // bool_to_string
    let s2: &str = bool_to_string(true);
    println_str(s2);
    if !str_eq(s2, "true") { return 2; }

    let s3: &str = bool_to_string(false);
    println_str(s3);
    if !str_eq(s3, "false") { return 3; }

    // i64_to_string
    let big: i64 = 1000000000000;
    let s4: &str = i64_to_string(big);
    println_str(s4);
    if !str_eq(s4, "1000000000000") { return 4; }

    0
}
