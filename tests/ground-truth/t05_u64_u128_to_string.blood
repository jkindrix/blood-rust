// Test: u64_to_string and u128_to_string conversion builtins
// EXPECT: 1000000
// EXPECT: 42
fn main() -> i32 {
    let s1: &str = u64_to_string(1000000);
    println_str(s1); // 1000000
    if !str_eq(s1, "1000000") { return 1; }

    let s2: &str = u128_to_string(42);
    println_str(s2); // 42
    if !str_eq(s2, "42") { return 2; }

    0
}
