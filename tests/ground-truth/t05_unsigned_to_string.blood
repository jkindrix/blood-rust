// Test: unsigned integer to string conversion builtins
// EXPECT: 255
// EXPECT: 65535
// EXPECT: 42
fn main() -> i32 {
    // u8_to_string
    let s1: &str = u8_to_string(255);
    println_str(s1); // 255
    if !str_eq(s1, "255") { return 1; }

    // u16_to_string
    let s2: &str = u16_to_string(65535);
    println_str(s2); // 65535
    if !str_eq(s2, "65535") { return 2; }

    // u32_to_string
    let s3: &str = u32_to_string(42);
    println_str(s3); // 42
    if !str_eq(s3, "42") { return 3; }

    0
}
