// Test: vec! macro creates array values
// EXPECT: 6
// EXPECT: 3
fn main() -> i32 {
    let v: [i32; 3] = vec![1, 2, 3];

    let sum: i32 = v[0] + v[1] + v[2];
    println_int(sum); // 6
    if sum != 6 { return 1; }

    let len: i32 = 3;
    println_int(len); // 3
    if len != 3 { return 2; }

    0
}
