// COMPILE_FAIL
// Test: using a value after it has been moved should fail
struct Data {
    value: i32,
}

fn consume(d: Data) -> i32 {
    d.value
}

fn main() -> i32 {
    let d: Data = Data { value: 42 };
    let a: i32 = consume(d);
    // d has been moved, using it again should fail
    let b: i32 = consume(d);
    0
}
