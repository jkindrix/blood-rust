// Test: && in while loop condition with guard pattern (BUG-011)
// Verifies that while j >= 0 && v[j] > threshold doesn't access v[-1].
// EXPECT: 20 DONE
fn main() -> i32 {
    let mut v: Vec<i64> = Vec::new();
    v.push(10);
    v.push(20);
    v.push(30);

    let mut j: i64 = 1;
    while j >= 0 && v[j as usize] > 15 {
        print_i64(v[j as usize]);
        print(" ");
        j = j - 1;
    }
    print("DONE\n");
    0
}
